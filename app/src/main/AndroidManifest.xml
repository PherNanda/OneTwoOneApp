<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.app.onetwoone">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.android.app.onetwoone.OTwoOApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_n"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_n_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.OneTwoOneApp"
        android:usesCleartextTraffic="true">
        <activity android:name="com.android.app.onetwoone.ui.view.DetailActivity"/>
        <activity android:name="com.android.app.onetwoone.ui.view.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>